{
    "proc sql":{
        "prefix": "sql",
        "body":[
            "PROC SQL noprint;",
            "\tcreate table $1 as",
            "\tselect $2",
            "\tfrom $3;",
            "QUIT;\n"
        ],
        "description": "proc sql"
    },
    "proc contents":{
        "prefix": "contents",
        "body":[
            "PROC CONTENTS data=$1;",
            "RUN;\n"
        ],
        "description": "proc contents"
    },
    "proc datasets":{
        "prefix": "datasets",
        "body":[
            "PROC DATASETS lib=$1 memtype=data nolist nodetails kill",
            "RUN;\nQUIT;\n"
        ],
        "description": "proc datasets"
    },
    "proc export":{
        "prefix": "export",
        "body":[
            "PROC EXPORT data=$1 outfile=\"$2\" DBMS=$3 replace;",
            "\tputnames=NO;",
            "RUN;\n"
        ],
        "description": "proc datasets"
    },
    "proc print":{
        "prefix": "print",
        "body":[
            "PROC PRINT data=$1;",
            "RUN;\n"
        ],
        "description": "proc print"
    },
    "proc rank":{
        "prefix": "rank",
        "body":[
            "PROC RANK data=$1 out=$2;",
            "\tvar $3;",
            "\tranks $4;",
            "RUN;\n"
        ],
        "description": "proc rank"
    },
    "proc freq":{
        "prefix": "freq",
        "body":[
            "PROC FREQ data=$1;",
            "\ttables $2/nocol nocum norow;",
            "RUN;\n"
        ],
        "description": "proc freq"
    },
    "proc corr":{
        "prefix": "corr",
        "body":[
            "PROC CORR data=$1;",
            "\tvar $2;",
            "RUN;\n"
        ],
        "description": "proc freq"
    },
    "proc univariate":{
        "prefix": "univariate",
        "body":[
            "PROC UNIVARIATE data=$1 noprint;",
            "\tvar $2;",
            "RUN;\n"
        ],
        "description": "proc univariate"
    }
}