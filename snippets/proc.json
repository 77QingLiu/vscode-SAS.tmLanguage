{
    "proc sql":{
        "prefix": "sql",
        "body":[
            "PROC SQL noprint;",
            "\tcreate table $1 as",
            "\tselect $2",
            "\tfrom $3;",
            "QUIT;\n"
        ],
        "description": "proc sql"
    },
    "proc contents":{
        "prefix": "contents",
        "body":[
            "PROC CONTENTS data=$1;",
            "RUN;\n"
        ],
        "description": "proc contents"
    },
    "proc datasets":{
        "prefix": "datasets",
        "body":[
            "PROC DATASETS lib=$1 memtype=data nolist nodetails kill",
            "RUN;\nQUIT;\n"
        ],
        "description": "proc datasets"
    },
    "proc import xlsx":{
        "prefix": "import xlsx",
        "body":[
            "PROC IMPORT",
                "\tdatafile=\"$1\"",
                "\tout=$2",
                "\tdbms=xlsx replace",
                "\tsheet=\"$3\";",
            "RUN;\n"
        ],
        "description": "proc import xlsx"
    },
    "proc import csv":{
        "prefix": "import csv",
        "body":[
            "PROC IMPORT",
                "\tdatafile=\"$1\"",
                "\tout=$2",
                "\tdbms=csv replace;",
            "RUN;\n"
        ],
        "description": "proc import csv"
    },
    "proc export":{
        "prefix": "export",
        "body":[
            "PROC EXPORT data=$1 outfile=\"$2\" DBMS=$3 replace;",
            "\tputnames=NO;",
            "RUN;\n"
        ],
        "description": "proc export"
    },
    "proc print":{
        "prefix": "print",
        "body":[
            "PROC PRINT data=$1;",
            "RUN;\n"
        ],
        "description": "proc print"
    },
    "proc rank":{
        "prefix": "rank",
        "body":[
            "PROC RANK data=$1 out=$2;",
            "\tvar $3;",
            "\tranks $4;",
            "RUN;\n"
        ],
        "description": "proc rank"
    },
    "proc freq":{
        "prefix": "freq",
        "body":[
            "PROC FREQ data=$1;",
            "\ttables $2/nocol nocum norow;",
            "RUN;\n"
        ],
        "description": "proc freq"
    },
    "proc corr":{
        "prefix": "corr",
        "body":[
            "PROC CORR data=$1;",
            "\tvar $2;",
            "RUN;\n"
        ],
        "description": "proc freq"
    },
    "proc univariate":{
        "prefix": "univariate",
        "body":[
            "PROC UNIVARIATE data=$1 noprint;",
            "\tvar $2;",
            "RUN;\n"
        ],
        "description": "proc univariate"
    },
    "proc anova":{
        "prefix": "anova",
        "body":[
            "PROC ANOVA data=$1;",
            "\tclass $2;",
            "\tmodel $3;",
            "RUN;\n"
        ],
        "description": "proc anova"
    },
    "proc cluster":{
        "prefix": "cluster",
        "body":[
            "PROC CLUSTER data=$1 method=${2|AVERAGE,CENTROID,COMPLETE,DENSITY,EML,FLEXIBLE,MCQUITTY,MEDIAN,SINGLE,TWOSTAGE,WARD|};",
            "\tvar $3;",
            "\tid $4;",
            "RUN;\n"
        ],
        "description": "proc cluster"
    },
    "proc factor":{
        "prefix": "factor",
        "body":[
            "PROC FACTOR data=$1 simple corr;",
            "RUN;\n"
        ],
        "description": "proc factor"
    },
    "proc surveyselect":{
        "prefix": "surveyselect",
        "body":[
            "PROC SURVEYSELECT data=$1 out=$2 method=${3|BERNOULLI,POISSON,PPS,BREWER,MURTHY,SAMPFORD,CHROMY,CHROMY,SRS,URS|};",
            "\tstrata $4;",
            "RUN;\n"
        ],
        "description": "proc surveyselect"
    }, 
    "proc reg":{
        "prefix": "reg",
        "body":[
            "PROC REG data=$1;",
            "\tmodel $2;",
            "RUN;\n"
        ],
        "description": "proc reg"
    },
    "proc logistic":{
        "prefix": "logistic",
        "body":[
            "PROC LOGISTIC data=$1;",
            "\tclass $2;",
            "\tmodel $3;",
            "RUN;\n"
        ],
        "description": "proc logistic"
    },
    "proc transpose":{
        "prefix": "transpose",
        "body":[
            "PROC TRANSPOSE data=$1 out=$2;",
            "\tby $3;",
            "\tvar $4;",
            "RUN;\n"
        ],
        "description": "proc transpose"
    },
    "proc lifetest":{
        "prefix": "lifetest",
        "body":[
            "PROC LIFETEST data=$1 outtest=$2;",
            "\ttime $3;",
            "\tstrata $4;",
            "RUN;\n"
        ],
        "description": "proc lifetest"
    },
    "proc fastclus":{
        "prefix": "fastclus",
        "body":[
            "PROC FASTCLUS data=$1 out=$2 maxc=$3 maxiter=$4;",
            "\tvar $5;",
            "RUN;\n"
        ],
        "description": "proc fastclus"
    },
    "proc glm":{
        "prefix": "glm",
        "body":[
            "PROC GLM data=$1;",
            "\tclass $2;",
            "\tmodel $3;",
            "RUN;\n"
        ],
        "description": "proc glm"
    },
    "proc mixed":{
        "prefix": "mixed",
        "body":[
            "PROC MIXED data=$1;",
            "\tclass $2;",
            "\tmodel $3;",
            "\trandom $4;",
            "RUN;\n"
        ],
        "description": "proc mixed"
    },
    "proc npar1way":{
        "prefix": "npar1way",
        "body":[
            "PROC NPAR1WAY data=$1;",
            "\tclass $2;",
            "\tvar $3;",
            "RUN;\n"
        ],
        "description": "proc npar1way"
    },
    "proc ttest":{
        "prefix": "ttest",
        "body":[
            "PROC TTEST data=$1;",
            "\tclass $2;",
            "\tvar $3;",
            "RUN;\n"
        ],
        "description": "proc ttest"
    },
    "proc sgrender":{
        "prefix": "sgrender",
        "body":[
            "PROC SGRENDER data=$1 template=$2;",
            "RUN;\n"
        ],
        "description": "proc sgrender"
    }
}