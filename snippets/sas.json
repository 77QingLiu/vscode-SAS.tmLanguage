{
    "Print to console": {
        "prefix": "data",
        "body": [
            "DATA $1;",
            "\t$2",
            "RUN;"
        ],
        "description": "data step"
    },
    "line comments": {
        "prefix": "coli",
        "body": [
            "*---------------------------------------------- $1 ------------------------------------------------;"
        ],
        "description": "line comments"
    },
    "section comments": {
        "prefix": "cosc",
        "body": [
            "*--------------------------------------------------------------------------------------------------;",
            "*                                           $1",
            "*--------------------------------------------------------------------------------------------------;"
        ],
        "description": "section comments"
    }
}