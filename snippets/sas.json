{
    "data step": {
        "prefix": "data",
        "body": [
            "DATA $1;",
            "\t$2",
            "RUN;\n"
        ],
        "description": "data step"
    },
    "line comments": {
        "prefix": "coli",
        "body": [
            "*---------------------------------------------- $1 ------------------------------------------------;"
        ],
        "description": "line comments"
    },
    "section comments": {
        "prefix": "cosc",
        "body": [
            "*--------------------------------------------------------------------------------------------------;",
            "*                                           $1",
            "*--------------------------------------------------------------------------------------------------;"
        ],
        "description": "section comments"
    },
    "proc format":{
        "prefix": "fmt",
        "body":[
            "PROC FORMAT;",
            "\tvalue ${1:txtfmt}",
            "\t\"$2\" = \"$3\"",
            "\t\"$4\" = \"$5\"",
            "\t\"$6\" = \"$7\"",
            "\t;",
            "RUN;\n$0"
        ],
        "description": "proc fromat"
    }
}