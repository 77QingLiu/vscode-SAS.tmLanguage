{
    "name": "sas-syntax",
    "displayName": "SAS-Syntax",
    "description": "syntax highlihter for SAS",
    "version": "0.0.1",
    "publisher": "qing",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "SAS",
            "aliases": ["SAS", "Statistical Analysis System"],
            "extensions": [".sas"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "SAS",
            "scopeName": "source.sas",
            "path": "./syntaxes/sas.tmLanguage.json"
        }],
        "themes": [
            {
                "label": "SAS Theme",
                "uiTheme": "vs",
                "path": "./themes/SAS theme-color-theme.json"
            }
        ],
        "snippets": [
            {
                "language": "SAS",
                "path": "./snippets/sas.json"
            },
            {
                "language": "SAS",
                "path": "./snippets/function.json"
            },
            {
                "language": "SAS",
                "path": "./snippets/proc.json"
            }
        ]
    }
}