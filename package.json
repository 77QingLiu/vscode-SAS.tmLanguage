{
    "name": "sas-syntax",
    "displayName": "SAS-Syntax",
    "description": "syntax highlihter for SAS",
    "version": "1.0.5",
    "publisher": "77qingliu",
    "repository": "https://github.com/77QingLiu/vscode-SAS.tmLanguage",
    "icon": "icon.png",
    "homepage": "https://github.com/77QingLiu/vscode-SAS.tmLanguage",
    "author": {
        "name": "Qing Liu",
        "url": "https://github.com/77qingliu"
    },
    "galleryBanner": {
        "color": "#ADD8E6",
        "theme": "light"
    },
    "bugs": {
        "url": "https://github.com/pvparuchuri/vscode-SAS.tmLanguage/issues"
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "SAS",
                "aliases": [
                    "SAS",
                    "Statistical Analysis System"
                ],
                "extensions": [
                    ".sas"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "SAS",
                "scopeName": "source.sas",
                "path": "./syntaxes/sas.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "SAS Theme",
                "uiTheme": "vs",
                "path": "./themes/SAS theme-color-theme.json"
            }
        ],
        "snippets": [
            {
                "language": "SAS",
                "path": "./snippets/sas.json"
            },
            {
                "language": "SAS",
                "path": "./snippets/function.json"
            },
            {
                "language": "SAS",
                "path": "./snippets/proc.json"
            }
        ]
    }
}